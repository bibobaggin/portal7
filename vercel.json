{
  "routes": [
    {
      "src": "/([^/.]*)\\.html",
      "dest": "/$1",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "/(.*)/",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*/",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
